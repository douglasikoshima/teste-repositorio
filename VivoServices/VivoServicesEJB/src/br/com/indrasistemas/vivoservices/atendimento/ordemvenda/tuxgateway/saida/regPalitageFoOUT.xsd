<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="msg" type="msgType"/>
	<xs:complexType name="msgType">
		<xs:sequence>
			<xs:element name="msgHdr" type="msgHdrType"/>
			<xs:element name="msgBody" type="msgBodyType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="msgHdrType">
		<xs:sequence>
			<xs:element name="user" type="xs:string"/>
			<xs:element name="service" type="xs:string"/>
			<xs:element name="subSystem" type="xs:string" minOccurs="0"/>
			<xs:element name="serverElapsedTime" type="xs:int" minOccurs="0"/>
			<xs:element name="statusCode" type="xs:string" minOccurs="0"/>
			<xs:element name="statusText" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="msgBodyType">
		<xs:sequence>
			<xs:element name="AtendimentoVO" type="AtendimentoVOType"/>
		</xs:sequence>
	</xs:complexType>

	<!--  <xs:complexType name="msgBodyType">
		<xs:sequence>
			<xs:element name="AtendimentoVO" type="AtendimentoVOType"/>
		</xs:sequence>
	</xs:complexType> -->

	<xs:complexType name="AtendimentoVOType">
		<xs:sequence>
			<xs:element name="nrProtocolo" type="xs:string"/>
			<xs:element name="idAtendimento" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
