<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- 
	Códigos de erro para base de Serviços Remotos (Ex. Legados), classificadas segundo o seu tipo.
-->
<config>
	<com>
		<indra>
			<service>
				<eai>
					<delegate>
						<exception>
							<UnknownException>100</UnknownException>
							<TimeoutException>101</TimeoutException>
						</exception>
					</delegate>
				</eai>
			</service>
			<framework>
				<service>
					<BusinessException>9999</BusinessException>
					<delegate>
						<BusinessDelegateException>9998</BusinessDelegateException>
					</delegate>
				</service>
			</framework>
		</indra>
	</com>
</config>