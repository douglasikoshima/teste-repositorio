<?xml version="1.0" encoding="ISO-8859-1" ?>
<config>
	<remote>
		<java>
		  <naming>
		    <security>
		    	<principal>weblogic</principal>
		    	<credentials>weblogic</credentials>
		    </security>
		    <provider>
		    	<url>
		    		t3://localhost:7001
		    	</url>
		    	<name>
			    	cgServer
		    	</name>
		    </provider>
		    <factory>
		    	<initial>
		    		weblogic.jndi.WLInitialContextFactory
		    	</initial>
		    </factory>
		  </naming>
		</java>
	</remote>
</config>