              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Produto" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="idProduto" type="xsd:long"/>
