<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.vivo.com.br/SN/CatalogoProduto" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:cwebPaginacao="http://www.vivo.com.br/SN/CatalogoProduto">
    <xsd:include schemaLocation="PaginacaoOut.xsd"/>
<xsd:element name="ResultadoBuscarListaProdutoSemCodigoAssociado">
