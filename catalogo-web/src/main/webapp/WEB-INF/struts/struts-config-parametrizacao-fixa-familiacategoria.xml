<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
    <data-sources />
    
	<form-beans>
		<form-bean name="familiaCategoriaForm" type="br.com.vivo.catalogoPRS.pageflows.shared.form.FamiliaCategoriaForm" />
	</form-beans>
	
	<global-forwards/>
	
	<action-mappings>
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/FamiliaCategoriaAction" 
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="begin" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/search" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="search" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/findCategorias" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="findCategorias" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/reccordFamilia" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="reccordFamilia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/reccordCategoria" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="reccordCategoria" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/removeFamilia" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="removeFamilia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/removeCategoria" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="removeCategoria" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/changeStatusFamilia" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="changeStatusFamilia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/familiacategoria/changeStatusCategoria" name="familiaCategoriaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.familiacategoria.FamiliaCategoriaAction" parameter="changeStatusCategoria" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/familiaCategoria/familiaCategoria.jsp"/>
     </action>
	</action-mappings>
	
	<message-resources key="messages" parameter="catalogoprs_messages" null="false"/>
	
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/struts/validator-rules.xml, /WEB-INF/struts/validation.xml" />
	</plug-in>
</struts-config>



 
