        type="br.com.vivo.catalogoPRS.pageflows.param.canalvendaagrupador.canalvenda.CanalVendaAction" parameter="salvar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/canalvendaagrupador/canalvenda/canalvenda.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/canalvendaagrupador/canalvenda/alternarInDisponivel" name="canalVendaAgrupadorForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.canalvendaagrupador.canalvenda.CanalVendaAction" parameter="alternarInDisponivel" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/canalvendaagrupador/canalvenda/canalvenda.jsp"/>
