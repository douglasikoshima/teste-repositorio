     <action path="/br/com/vivo/catalogoPRS/pageflows/param/canalvendaagrupador/CanalVendaAgrupadorAction" 
        type="br.com.vivo.catalogoPRS.pageflows.param.canalvendaagrupador.CanalVendaAgrupadorAction" parameter="begin" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/canalvendaagrupador/canalvendaagrupador.jsp"/>
     </action>
     <!-- Fim CanalVendaAgrupadorAction -->
     
	<!-- Inicio CanalVendaAction -->
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/canalvendaagrupador/canalvenda/CanalVendaAction" 
        type="br.com.vivo.catalogoPRS.pageflows.param.canalvendaagrupador.canalvenda.CanalVendaAction" parameter="begin" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/canalvendaagrupador/canalvenda/canalvenda.jsp"/>
     </action>
     
