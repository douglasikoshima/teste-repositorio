     <action path="/br/com/vivo/catalogoPRS/pageflows/param/canalvendaagrupador/agrupador/remover" name="canalVendaAgrupadorForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.canalvendaagrupador.agrupador.AgrupadorAction" parameter="remover" scope="request" validate="false">
