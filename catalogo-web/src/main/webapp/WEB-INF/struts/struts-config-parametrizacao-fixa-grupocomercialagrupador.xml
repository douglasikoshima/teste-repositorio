     </action>     
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/canalvendaagrupador/canalvenda/criarNovo" name="canalVendaAgrupadorForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.canalvendaagrupador.canalvenda.CanalVendaAction" parameter="criarNovo" scope="request" validate="false">
