<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<data-sources/>
	
	<form-beans>
		<form-bean name="caracteristicaForm" type="br.com.vivo.catalogoPRS.pageflows.shared.form.CaracteristicaForm"/>
	</form-beans>
	
	<global-forwards/>
	
	<action-mappings>
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/CaracteristicaAction" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="begin" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/search" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="search" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/create" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="create" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/edit" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="edit" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/save" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="save" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/saveCaracteristicaByValores" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="saveCaracteristicaByValores" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/remove" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="remove" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaList.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/findValores" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="findValores" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaListPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/abrirValores" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="abrirValores" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaFormPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/saveValores" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="saveValores" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaFormPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/loadValor" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="loadValor" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaFormPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/removeValores" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="removeValores" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaFormPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/verificarAssociacaoExclusao" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="verificarAssociacaoExclusao" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaConfirmacaoExclusaoPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/verificarAssociacaoAlteracao" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="verificarAssociacaoAlteracao" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/caracteristica/caracteristicaConfirmacaoAlteracaoPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/verificarAssociacaoExclusaoValorCaracteristica" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="verificarAssociacaoExclusaoValorCaracteristica" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaConfirmacaoExclusaoPopup.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/param/caracteristica/verificarAssociacaoAlteracaoValorCaracteristica" name="caracteristicaForm"
			type="br.com.vivo.catalogoPRS.pageflows.param.caracteristica.CaracteristicaAction" parameter="verificarAssociacaoAlteracaoValorCaracteristica" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pages/valorCaracteristica/valorCaracteristcaConfirmacaoAlteracaoPopup.jsp"/>
		</action>
	</action-mappings>
	
	<message-resources key="messages" parameter="catalogoprs_messages" null="false"/>
	
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/struts/validator-rules.xml, /WEB-INF/struts/validation.xml" />
	</plug-in>
</struts-config>