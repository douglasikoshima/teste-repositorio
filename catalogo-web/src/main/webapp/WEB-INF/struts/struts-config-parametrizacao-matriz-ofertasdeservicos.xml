<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
		<data-sources />

		<form-beans>
			<form-bean name="ofertaServicosMatrizOfertaForm" type="br.com.vivo.catalogoPRS.pageflows.shared.form.OfertaServicosMatrizOfertaForm" />			
		</form-beans>
		
		<action-mappings>
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/OfertaServicosMatrizOfertaAction" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="begin" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/ofertaServicosMatrizOferta.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/pesquisarOfertaServicosMatriz" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="pesquisarOfertaServicosMatriz" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/resultadoPesquisaOfertaServicos.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/criarOfertaServicosMatriz" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="criarOfertaServicosMatriz" scope="request" validate="false">
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/listarServicosAssociados" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="listarServicosAssociados" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/popupServicosAssociados.jsp"/>
			</action>

			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/abrirAlterarOfertaServicosMatriz" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="abrirAlterarOfertaServicosMatriz" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/abrirAlterarOfertaServicos.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/alterarOfertaServicosMatriz" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="alterarOfertaServicosMatriz" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/resultadoPesquisaOfertaServicos.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/abrirPopupExcluirOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="abrirPopupExcluirOfertaServico" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/popupExcluirOfertaServico.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/excluirOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="excluirOfertaServico" scope="request" validate="false">
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/abrirServicosAssociadosOfertaServicosMatriz" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="abrirServicosAssociadosOfertaServicosMatriz" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/servicosAssociadosOfertaServicos.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/abrirPopupExcluirAssociacaoServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="abrirPopupExcluirAssociacaoServico" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/apagarAssociacaoServicoOfertaServico.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/excluirServicoOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="excluirServicoOfertaServico" scope="request" validate="false">
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/abrirPopupNovaAssociacao" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="abrirPopupNovaAssociacao" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/popupAssociacaoDeServicos.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/pesquisarServicosDisponiveisParaAssociacao" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="pesquisarServicosDisponiveisParaAssociacao" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/resultadoPesquisaServicoDispAssociacao.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/efetivarAssociasaoServicoOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="efetivarAssociasaoServicoOfertaServico" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/resultadoPesquisaOfertaServicos.jsp"/>
				<forward name="error" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/resultadoPesquisaServicoDispAssociacao.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/validarServicoOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="validarServicoOfertaServico" scope="request" validate="false">
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/incompatibilidadeAssociacaoServicoOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="incompatibilidadeAssociacaoServicoOfertaServico" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/popupErroAssociacaoServicoOfertaServico.jsp"/>
			</action>
			
			<action path="/br/com/vivo/catalogoPRS/pageflows/param/matrizOferta/ofertaServicos/confirmAssociacaoServicoOfertaServico" name="ofertaServicosMatrizOfertaForm"
				type="br.com.vivo.catalogoPRS.pageflows.param.matrizOferta.ofertaServicos.OfertaServicosMatrizOfertaAction" parameter="confirmAssociacaoServicoOfertaServico" scope="request" validate="false">
				<forward name="success" path="/WEB-INF/pageflows/param/matrizOferta/ofertaServicos/popupConfirmarAssociacaoServicoOfertaServico.jsp"/>
			</action>
		</action-mappings>
		
		<message-resources key="messages" parameter="catalogoprs_messages" null="false"/>
	
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/struts/validator-rules.xml, /WEB-INF/struts/validation.xml" />
	</plug-in>

</struts-config>