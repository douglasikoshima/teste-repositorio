<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<data-sources />

	<form-beans>	
     <form-bean name="associacaoFrequenciaForm" type="br.com.vivo.catalogoPRS.pageflows.shared.form.AssociacaoFrequenciaForm"/>
	</form-beans>      

	<global-forwards/> 
	
    <action-mappings>  
        
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequenciaAction" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="begin" scope="request" validate="false">
      <forward name="default" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequencia.jsp"/>
     </action>
  
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/apagarAssociacaoTecnologiaFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="apagarAssociacaoTecnologiaFrequencia" scope="request" validate="false">
      <forward name="default" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/popupApagarTecnologiaFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="popupApagarTecnologiaFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/ApagarAssociacaoTecnologiaFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/addAssociacaoTecnologiaFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="addAssociacaoTecnologiaFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/valoresFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="valoresFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/popupValoresFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/alterarFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="alterarFrequencia" scope="request" validate="false">
      <forward name="default" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/carregarAlterarTecnologiaFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="carregarAlterarTecnologiaFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AlterarAssociacaoTecnologiaFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/alterarValorTecnologiaTipoFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="alterarValorTecnologiaTipoFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/popupDesassociarValorFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/confirmarValoresTecnologiaTipoFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="confirmarValoresTecnologiaTipoFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequencia.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/produtos/associacaoTecnologiaFrequencia/desassociarValorFrequenciaTecnologiaTipoFrequencia" name="associacaoFrequenciaForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.produtos.associacaoTecnologiaFrequencia.AssociacaoTecnologiaFrequenciaAction" parameter="desassociarValorFrequenciaTecnologiaTipoFrequencia" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pageflows/param/produtos/associacaoTecnologiaFrequencia/AssociacaoTecnologiaFrequencia.jsp"/>
     </action>
   
    </action-mappings>           
     
    <message-resources key="messages" parameter="catalogoprs_messages" null="false"/>
       
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/struts/validator-rules.xml, /WEB-INF/struts/validation.xml" />
	</plug-in>
</struts-config>