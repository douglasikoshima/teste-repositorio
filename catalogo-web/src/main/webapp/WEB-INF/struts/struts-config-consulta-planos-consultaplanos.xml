<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<data-sources/>
	
	<form-beans>
		<form-bean name="consultaPlanosForm" type="br.com.vivo.catalogoPRS.pageflows.shared.form.ConsultaPlanosForm"></form-bean>
	</form-beans>
	
	<global-forwards/>
	
	<action-mappings>
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/ConsultaPlanosAction" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="begin" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/consultaPlanos.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/pesquisaSimplesPlanos" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="pesquisaSimplesPlanos" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/resultadoPesquisaPlanos.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarUFs" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarUFs" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionarUFs.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarTiposCliente" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarTiposCliente" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarPlataformas" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarPlataformas" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarCanais" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarCanais" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarCarteiras" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarCarteiras" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarSegmentos" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarSegmentos" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarUFSAvancada" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarUFSAvancada" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarDDDs" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarDDDs" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupSelecionar.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/popupServicos" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="popupServicos" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupServicosPlano.jsp"/>
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/popupUFs" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="popupUFs" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupUFsPlano.jsp"/>
		</action>
		
		<!-- exportarVariaveis -->
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/exportarVariaveis" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="exportarVariaveis" scope="request" validate="false">
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/listarVariaveisPlano" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="listarVariaveisPlano" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/variaveisPlano.jsp"/>
		</action>
		
		<!-- exportarPlanos -->
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/exportarPlanos" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="exportarPlanos" scope="request" validate="false">
		</action>
		
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/popupAlterarDescricaoPlano" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="popupAlterarDescricaoPlano" scope="request" validate="false">
		 <forward name="success" path="/WEB-INF/pageflows/consulta/planos/consultaPlanos/popupAlterarDescricaoPlano.jsp"/>
		</action>
		
		<!-- alterarDescricaoPlano -->
		<action path="/br/com/vivo/catalogoPRS/pageflows/consulta/planos/consultaPlanos/alterarDescricaoPlano" name="consultaPlanosForm"
			type="br.com.vivo.catalogoPRS.pageflows.consulta.planos.consultaPlanos.ConsultaPlanosAction" parameter="alterarDescricaoPlano" scope="request" validate="false">
		</action>
	</action-mappings>
	
	<message-resources key="messages" parameter="catalogoprs_messages" null="false"/>
	
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/struts/validator-rules.xml, /WEB-INF/struts/validation.xml" />
	</plug-in>
</struts-config>