				name="parametrizacaoProdutoModeloForm">
				<forward name="success" path="/WEB-INF/pages/modelo/parametrizacaoModelo.jsp"/>
			
			</action>
			
			<action 
				path="/br/com/vivo/catalogoPRS/pageflows/param/modelo/getFabricante"
				type="br.com.vivo.catalogoPRS.pageflows.param.modelo.ModeloAction"
				parameter="getFabricante" scope="request" validate="false"
				name="parametrizacaoProdutoModeloForm">
				<forward name="success" path="/WEB-INF/pages/modelo/parametrizacaoModelo.jsp"/>
	 		
			</action>
			
			<action 
				path="/br/com/vivo/catalogoPRS/pageflows/param/modelo/getModelo"
				type="br.com.vivo.catalogoPRS.pageflows.param.modelo.ModeloAction"
