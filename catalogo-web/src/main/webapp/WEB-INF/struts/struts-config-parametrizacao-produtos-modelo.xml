			</action>
			
			
			<action 
				path="/br/com/vivo/catalogoPRS/pageflows/param/modelo/obterCaracteristicas"
				type="br.com.vivo.catalogoPRS.pageflows.param.modelo.ModeloAction"
				parameter="obterCaracteristicas" scope="request" validate="false"
				name="parametrizacaoProdutoModeloForm">
				<forward name="success" path="/WEB-INF/pages/modelo/parametrizacaoModelo.jsp"/>
			
