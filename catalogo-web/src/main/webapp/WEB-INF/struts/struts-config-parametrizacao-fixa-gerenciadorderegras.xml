<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
    <data-sources />
    
	<form-beans>
		<form-bean name="gerenciadorRegrasAbaFiltroForm" type="br.com.vivo.catalogoPRS.pageflows.shared.form.GerenciadorRegrasAbaFiltroForm" />
	</form-beans>
	
	<global-forwards/>
	
	<action-mappings>
	<!-- Inicio GerenciadorRegrasConfiguracaoFiltro1Action -->
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/filtro1/GerenciadorRegrasConfiguracaoFiltro1Action" 
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.filtro1.GerenciadorRegrasConfiguracaoFiltro1Action" parameter="begin" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/filtro1/gerenciadorregrasfiltro1.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/filtro1/salvar" name="gerenciadorRegrasAbaFiltroForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.filtro1.GerenciadorRegrasConfiguracaoFiltro1Action" parameter="salvar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/filtro1/gerenciadorregrasfiltro1.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/filtro1/pesquisar" name="gerenciadorRegrasAbaFiltroForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.filtro1.GerenciadorRegrasConfiguracaoFiltro1Action" parameter="pesquisar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/filtro1/gerenciadorregrasfiltro1.jsp"/>
     </action>
     <!-- Fim GerenciadorRegrasConfiguracaoFiltro1Action -->
     
     <!-- Inicio GerenciadorRegrasConfiguracaoFiltro2Action -->
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/filtro2/GerenciadorRegrasConfiguracaoFiltro2Action" 
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.filtro2.GerenciadorRegrasConfiguracaoFiltro2Action" parameter="begin" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/filtro2/gerenciadorregrasfiltro2.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/filtro2/salvar" name="gerenciadorRegrasAbaFiltroForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.filtro2.GerenciadorRegrasConfiguracaoFiltro2Action" parameter="salvar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/filtro2/gerenciadorregrasfiltro2.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/filtro2/pesquisar" name="gerenciadorRegrasAbaFiltroForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.filtro2.GerenciadorRegrasConfiguracaoFiltro2Action" parameter="pesquisar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/filtro2/gerenciadorregrasfiltro2.jsp"/>
     </action>
     <!-- Fim GerenciadorRegrasConfiguracaoFiltro2Action --> 
     
     <!-- Inicio GerenciadorRegrasConfiguracaoDocumentosAction -->
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/documentos/GerenciadorRegrasConfiguracaoDocumentosAction" 
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.documentos.GerenciadorRegrasConfiguracaoDocumentosAction" parameter="begin" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/documentos/gerenciadorregrasdocumentos.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/documentos/salvar" name="gerenciadorRegrasAbaFiltroForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.documentos.GerenciadorRegrasConfiguracaoDocumentosAction" parameter="salvar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/documentos/gerenciadorregrasdocumentos.jsp"/>
     </action>
     
     <action path="/br/com/vivo/catalogoPRS/pageflows/param/gerenciadorregras/documentos/pesquisar" name="gerenciadorRegrasAbaFiltroForm"
        type="br.com.vivo.catalogoPRS.pageflows.param.gerenciadorregras.documentos.GerenciadorRegrasConfiguracaoDocumentosAction" parameter="pesquisar" scope="request" validate="false">
      <forward name="success" path="/WEB-INF/pages/gerenciadorregras/documentos/gerenciadorregrasdocumentos.jsp"/>
     </action>          
	</action-mappings>
	<!-- Fim GerenciadorRegrasConfiguracaoDocumentosAction -->
	
    <message-resources key="messages" parameter="catalogoprs_messages" null="false"/>
	
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/struts/validator-rules.xml, /WEB-INF/struts/validation.xml" />
	</plug-in>
</struts-config>



 
