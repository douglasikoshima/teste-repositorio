<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
              "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
              "struts-config_1_2.dtd">
<struts-config>
    <form-beans>
        <form-bean name="formMenu" type="VOLE.Parametrizacao.ParametrizacaoController$FormMenu" />
        <form-bean name="parametrizacaoBannerForm" type="VOLE.ParametrizacaoBanner.formBeans.ParametrizacaoBannerForm" />
		<form-bean name="relatorioMVEForm" type="VOLE.relatorioMVE.RelatorioForm" />
        <form-bean name="manterGrupamentoForm" type="VOLE.manterGrupamento.ManterGrupamentoController$ManterGrupamentoForm" />
    </form-beans>
    <action-mappings>
	
        <!--  Relatorio MEU VIVO EMPRESAS -->
        <action path="/VOLE/relatorioMVE/begin" type="VOLE.relatorioMVE.RelatorioMVEController"
                name="relatorioMVEForm"  
           parameter="begin"
               scope="request">
            <forward name="success" path="/VOLE/relatorioMVE/index.jsp" />
        </action>      
        <action path="/VOLE/relatorioMVE/clean" type="VOLE.relatorioMVE.RelatorioMVEController" 
           parameter="clean"
               scope="request">
            <forward name="success" path="/VOLE/relatorioMVE/index.jsp" />
        </action>       
        <action path="/VOLE/relatorioMVE/pesquisar" type="VOLE.relatorioMVE.RelatorioMVEController"
                name="relatorioMVEForm" 
           parameter="pesquisar"
               scope="request">
            <forward name="success" path="/VOLE/relatorioMVE/tabelaRelatorio.jsp" />
        </action>        
        <action path="/VOLE/relatorioMVE/download" type="VOLE.relatorioMVE.RelatorioMVEController"
                name="relatorioMVEForm" 
           parameter="download"
               scope="request">
            <forward name="success" path="/VOLE/relatorioMVE/index.jsp" />
        </action>                         
        <!--  Relatorio MEU VIVO EMPRESAS -->	

        <!--action-mappings VOLE.Parametrizacao.ParametrizacaoController -->
        <action path="/VOLE/Parametrizacao/begin" type="VOLE.Parametrizacao.ParametrizacaoController" parameter="begin"
            scope="request">
            <forward name="success" path="/VOLE/Parametrizacao/index.jsp" />
        </action>
        <action path="/VOLE/Parametrizacao/abaFiltro" type="VOLE.Parametrizacao.ParametrizacaoController"
            parameter="abaFiltro" scope="request" name="formMenu">
            <forward name="success" path="/VOLE/Parametrizacao/abaFiltro.jsp" />
        </action>
        <action path="/VOLE/Parametrizacao/resultadoPesquisa" type="VOLE.Parametrizacao.ParametrizacaoController"
            parameter="resultadoPesquisa" scope="request" name="formMenu">
            <forward name="success" path="/VOLE/Parametrizacao/resultadoPesquisa.jsp" />
        </action>
        <action path="/VOLE/Parametrizacao/gravarParametro" type="VOLE.Parametrizacao.ParametrizacaoController"
            parameter="gravarParametro" scope="request" name="formMenu">
            <forward name="success" path="/VOLE/Parametrizacao/" />
        </action>
        <action path="/VOLE/Parametrizacao/manterAcesso" type="VOLE.Parametrizacao.ParametrizacaoController"
            parameter="manterAcesso" scope="request" name="formMenu">
            <forward name="success" path="/VOLE/Parametrizacao/ManterAcesso.jsp" />
        </action>
        <action path="/VOLE/Parametrizacao/alterarStatusMenu" type="VOLE.Parametrizacao.ParametrizacaoController"
            parameter="alterarStatusMenu" scope="request">
        </action>
        <action path="/VOLE/Parametrizacao/paginarPesquisa" type="VOLE.Parametrizacao.ParametrizacaoController"
            parameter="paginarPesquisa" scope="request" name="formMenu">
            <forward name="success" path="/VOLE/Parametrizacao/resultadoPesquisa.jsp" />
        </action>
        <!-- manter grupamento -->
        <action path="/VOLE/manterGrupamento/begin" type="VOLE.manterGrupamento.ManterGrupamentoController"
            parameter="begin" scope="request" name="manterGrupamentoForm">
            <forward name="success" path="/VOLE/manterGrupamento/manterGrupamento.jsp" />
        </action>
        <action path="/VOLE/manterGrupamento/listarGrupamentos" type="VOLE.manterGrupamento.ManterGrupamentoController"
            parameter="listarGrupamentos" scope="request" name="manterGrupamentoForm">
            <forward name="success" path="/VOLE/manterGrupamento/manterGrupamento.jsp" />
        </action>
        <action path="/VOLE/manterGrupamento/incluirGrupamento" type="VOLE.manterGrupamento.ManterGrupamentoController"
            parameter="incluirGrupamento" scope="request" name="manterGrupamentoForm">
            <forward name="success" path="/VOLE/manterGrupamento/incluirGrupamento.jsp" />
        </action>   
        <action path="/VOLE/manterGrupamento/alterarGrupamento" type="VOLE.manterGrupamento.ManterGrupamentoController"
            parameter="alterarGrupamento" scope="request" name="manterGrupamentoForm">
            <forward name="success" path="/VOLE/manterGrupamento/incluirGrupamento.jsp" />
        </action>  
        <action path="/VOLE/manterGrupamento/salvarGrupamento" type="VOLE.manterGrupamento.ManterGrupamentoController"
            parameter="salvarGrupamento" scope="request" name="manterGrupamentoForm">
            <forward name="success" path="/VOLE/manterGrupamento/listarGrupamentos.do" />
        </action>  
        <action path="/VOLE/manterGrupamento/removerGrupamento" type="VOLE.manterGrupamento.ManterGrupamentoController"
            parameter="removerGrupamento" scope="request" name="manterGrupamentoForm">
            <forward name="success" path="/VOLE/manterGrupamento/listarGrupamentos.do" />
        </action>
        <!-- manter grupamento fim -->
        <!--action-mappings VOLE.Parametrizacao.ParametrizacaoController end -->
        <!--action-mappings VOLE.ParametrizacaoBanner.ParametrizacaoBannerController -->
        <action path="/VOLE/ParametrizacaoBanner/begin" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="begin" scope="request" name="parametrizacaoBannerForm">
            <forward name="success" path="/VOLE/ParametrizacaoBanner/index.jsp" />
            <forward name="error" path="/error.jsp" />
        </action>
        <action path="/VOLE/ParametrizacaoBanner/beginCadastroBanner" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="beginCadastroBanner" scope="request" name="parametrizacaoBannerForm">
            <forward name="success" path="/VOLE/ParametrizacaoBanner/criarAlterarBanner.jsp" />
        </action>
        <action path="/VOLE/ParametrizacaoBanner/enviarArquivo" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="enviarArquivo" scope="request" name="parametrizacaoBannerForm">
            <forward name="success" path="/VOLE/ParametrizacaoBanner/retornoUpload.jsp" />
        </action>
        <action path="/VOLE/ParametrizacaoBanner/pesquisar" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="pesquisar" scope="request" name="parametrizacaoBannerForm">
            <forward name="successUpload" path="/VOLE/ParametrizacaoBanner/listaBanners.jsp" />
            <forward name="successAssociacao" path="/VOLE/ParametrizacaoBanner/listaRelacionamentos.jsp" />
        </action>
        <action path="/VOLE/ParametrizacaoBanner/associarDesassociarBanner" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="associarDesassociarBanner" scope="request" name="parametrizacaoBannerForm">
            <forward name="successAssociacao" path="/VOLE/ParametrizacaoBanner/listaBannersAssociacao.jsp" />
            <forward name="sucessDesassociacao" path="/VOLE/ParametrizacaoBanner/sucessDesassociacao.jsp" />
            <forward name="error" path="/error.jsp" />
        </action>
        <action path="/VOLE/ParametrizacaoBanner/salvarAssociacaoDesassociacaoBanner" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="salvarAssociacaoDesassociacaoBanner" scope="request" name="parametrizacaoBannerForm">
        </action>
        <action path="/VOLE/ParametrizacaoBanner/mostrarImagemBanner" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="mostrarImagemBanner" scope="request" name="parametrizacaoBannerForm">
            <forward name="success" path="/VOLE/ParametrizacaoBanner/imagemBanner.jsp" />
        </action>
        <action path="/VOLE/ParametrizacaoBanner/salvarCadastroAlteracao" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="salvarCadastroAlteracao" scope="request" name="parametrizacaoBannerForm">
        </action>
        <action path="/VOLE/ParametrizacaoBanner/buscarDadosBanner" type="VOLE.ParametrizacaoBanner.ParametrizacaoBannerController"
            parameter="buscarDadosBanner" scope="request" name="parametrizacaoBannerForm">
            <forward name="success" path="/VOLE/ParametrizacaoBanner/criarAlterarBanner.jsp" />
        </action>
    </action-mappings>
    <controller nocache="true" processorClass="com.indracompany.struts.annotations.RequestProcessor" />
    <message-resources parameter="com/indracompany/actions/ApplicationResource" />
</struts-config>