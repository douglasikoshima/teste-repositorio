<?xml version="1.0" encoding="UTF-8"?>
<weblogic-web-app xmlns="http://xmlns.oracle.com/weblogic/weblogic-web-app"
	xmlns:j2ee="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://xmlns.oracle.com/weblogic/weblogic-web-app http://xmlns.oracle.com/weblogic/weblogic-web-app/1.2/weblogic-web-app.xsd">
	<context-root>/FrontOfficeWeb</context-root>
	<description>Refactoring do Vivonet</description>
	<weblogic-version>10.3.4</weblogic-version>
	<session-descriptor>
		<timeout-secs>1800</timeout-secs>
		<invalidation-interval-secs>30</invalidation-interval-secs>
	</session-descriptor>
	<jsp-descriptor>
        <keepgenerated>true</keepgenerated>
		<verbose>true</verbose>
		<encoding>ISO-8859-1</encoding>
	</jsp-descriptor>
	<charset-params>
		<input-charset>
			<resource-path>/*</resource-path>
			<java-charset-name>ISO-8859-1</java-charset-name>
		</input-charset>
	</charset-params>
</weblogic-web-app>