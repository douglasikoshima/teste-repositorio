<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
              "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
              "struts-config_1_2.dtd">
<struts-config>
    <form-beans>
        <!--form-bean tracking.PrevisaoEntrega.PrevisaoEntregaController -->
        <form-bean name="previsaoEntregaForm" type="tracking.PrevisaoEntrega.PrevisaoEntregaController$TrackingForm" />
        <!--form-bean tracking.PrevisaoEntrega.PrevisaoEntregaController end -->
        <!--form-bean tracking.RelatoriosTracking.RelatoriosTrackingController -->
        <form-bean name="relatoriosTrackingForm" type="tracking.RelatoriosTracking.RelatoriosTrackingController$RelatoriosTrackingForm" />
        <!--form-bean tracking.RelatoriosTracking.RelatoriosTrackingController end -->
    </form-beans>
    <action-mappings>
        <!--action-mappings tracking.PrevisaoEntrega.PrevisaoEntregaController -->
        <action path="/tracking/PrevisaoEntrega/begin" type="tracking.PrevisaoEntrega.PrevisaoEntregaController"
            parameter="begin" scope="request">
            <forward name="success" path="/tracking/PrevisaoEntrega/index.jsp" />
        </action>
        <action path="/tracking/PrevisaoEntrega/pesquisar" type="tracking.PrevisaoEntrega.PrevisaoEntregaController"
            parameter="pesquisar" scope="request" name="previsaoEntregaForm">
            <forward name="success" path="/tracking/PrevisaoEntrega/index.jsp" />
        </action>
        <action path="/tracking/PrevisaoEntrega/gravar" type="tracking.PrevisaoEntrega.PrevisaoEntregaController"
            parameter="gravar" scope="request" name="previsaoEntregaForm">
            <forward name="success" path="/tracking/PrevisaoEntrega/pesquisar.do" />
        </action>
        <!--action-mappings tracking.PrevisaoEntrega.PrevisaoEntregaController end -->
        <!--action-mappings tracking.RelatoriosTracking.RelatoriosTrackingController -->
        <action path="/tracking/RelatoriosTracking/begin" type="tracking.RelatoriosTracking.RelatoriosTrackingController"
            parameter="begin" scope="request" name="relatoriosTrackingForm">
            <forward name="success" path="/tracking/RelatoriosTracking/index.jsp" />
            <forward name="error" path="/error.jsp" />
        </action>
        <action path="/tracking/RelatoriosTracking/getRelatorio" type="tracking.RelatoriosTracking.RelatoriosTrackingController"
            parameter="getRelatorio" scope="request" name="relatoriosTrackingForm">
            <forward name="relatorioConsolidado" path="/tracking/RelatoriosTracking/relatorioConsolidado.jsp" />
            <forward name="relatorioDetalhado" path="/tracking/RelatoriosTracking/relatorioDetalhado.jsp" />
            <forward name="error" path="/error.jsp" />
        </action>
        <!--action-mappings tracking.RelatoriosTracking.RelatoriosTrackingController end -->
    </action-mappings>
    <controller nocache="true" processorClass="com.indracompany.struts.annotations.RequestProcessor" />
    <!-- <controller processorClass="org.apache.struts.tiles.TilesRequestProcessor"/> -->
    <message-resources parameter="com/indracompany/actions/ApplicationResource" />
</struts-config>