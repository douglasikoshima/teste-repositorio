<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
              "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
              "struts-config_1_2.dtd">
<struts-config>
    <form-beans>
        <form-bean name="iniciaQuestionarioForm" type="questionario.QuestionarioController$IniciaQuestionarioForm" />
        <form-bean name="salvaAgendaAniversarioForm" type="questionario.QuestionarioController$SalvaAgendaAniversarioForm" />
    </form-beans>
    <action-mappings>
        <!-- /questionario -->
        <action path="/questionario/begin" type="questionario.QuestionarioController" parameter="begin" scope="request"
            name="historicoConsultarForm">
            <forward name="1" path="/questionario/questionarioFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/questionarioCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="finalizar" path="/questionario/finalizarQuestionario.do" />
            <forward name="questionarioVazio1" path="/questionario/questionarioVazioFidelAtendimento.jsp" />
            <forward name="questionarioVazio2" path="/questionario/questionarioVazioCmpAtendimento.jsp" />
            <forward name="questionarioVazio3" path="/questionario/questionarioVazioCmpSimulacao.jsp" />
            <forward name="questionarioVazio4" path="/questionario/questionarioVazioCmpAtendimento.jsp" />
            <forward name="errorFidel" path="/questionario/done.do" />
            <forward name="error" path="/error.jsp" />
        </action>
        <action path="/questionario/onReturnTelaInicial" type="questionario.QuestionarioController" parameter="onReturnTelaInicial"
            scope="request">
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="finalizar" path="/questionario/finalizarQuestionario.do" />
            <forward name="questionarioVazio2" path="/questionario/questionarioVazioCmpAtendimento.jsp" />
            <forward name="questionarioVazio4" path="/questionario/questionarioVazioCmpAtendimento.jsp" />
            <forward name="errorFidel" path="/questionario/done.do" />
            <forward name="error" path="/error.jsp" />
        </action>
        <action path="/questionario/done" type="questionario.QuestionarioController" parameter="done" scope="request">
            <forward name="done" path="/questionario/QuestionarioDone.do" />
        </action>
        <action path="/questionario/factoryRespostaForward" type="questionario.QuestionarioController" parameter="factoryRespostaForward"
            scope="request" name="iniciaQuestionarioForm">
            <forward name="respostaTexto" path="/questionario/respostaTexto.do" />
            <forward name="respostaRadio" path="/questionario/respostaRadio.do" />
            <forward name="respostaCheck" path="/questionario/respostaCheck.do" />
        </action>
        <action path="/questionario/respostaCheck" type="questionario.QuestionarioController" parameter="respostaCheck"
            scope="request" name="iniciaQuestionarioForm">
            <forward name="1" path="/questionario/questionarioFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/questionarioCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="finalizar" path="/questionario/finalizarQuestionario.do" />
            <forward name="regreso" path="/fidelizacao/regresoVoltar.do" />
        </action>
        <action path="/questionario/respostaRadio" type="questionario.QuestionarioController" parameter="respostaRadio"
            scope="request" name="iniciaQuestionarioForm">
            <forward name="1" path="/questionario/questionarioFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/questionarioCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="finalizar" path="/questionario/finalizarQuestionario.do" />
            <forward name="regreso" path="/fidelizacao/regresoVoltar.do" />
        </action>
        <action path="/questionario/respostaTexto" type="questionario.QuestionarioController" parameter="respostaTexto"
            scope="request" name="iniciaQuestionarioForm">
            <forward name="1" path="/questionario/questionarioFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/questionarioCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="finalizar" path="/questionario/finalizarQuestionario.do" />
            <forward name="regreso" path="/fidelizacao/regresoVoltar.do" />
        </action>
        <action path="/questionario/finalizarQuestionario" type="questionario.QuestionarioController" parameter="finalizarQuestionario"
            scope="request">
            <forward name="success" path="/questionario/done.do" />
            <forward name="error" path="/error.jsp" />
        </action>
        <action path="/questionario/showAndamento" type="questionario.QuestionarioController" parameter="showAndamento"
            scope="request">
            <forward name="1" path="/questionario/viewScriptFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/viewScriptCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/viewScriptCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/viewScriptCmpAtendimento.jsp" />
        </action>
        <action path="/questionario/voltarQuestionario" type="questionario.QuestionarioController" parameter="voltarQuestionario"
            scope="request">
            <forward name="1" path="/questionario/questionarioFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/questionarioCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="regreso" path="/fidelizacao/getLinhas.do" />
        </action>
        <action path="/questionario/salvaAgendaAniversario" type="questionario.QuestionarioController" parameter="salvaAgendaAniversario"
            scope="request" name="salvaAgendaAniversarioForm">
            <forward name="success" path="/questionario/dataAniversario.jsp" />
        </action>
        <action path="/questionario/dataAniversario" type="questionario.QuestionarioController" parameter="dataAniversario"
            scope="request">
            <forward name="success" path="/questionario/dataAniversario.jsp" />
        </action>
        <action path="/questionario/voltarNavegacaoScript" type="questionario.QuestionarioController" parameter="voltarNavegacaoScript"
            scope="request" name="XXXXXXXXXXXXXXXX">
            <forward name="1" path="/questionario/questionarioFidelAtendimento.jsp" />
            <forward name="2" path="/questionario/questionarioCmpAtendimento.jsp" />
            <forward name="3" path="/questionario/questionarioCmpSimulacao.jsp" />
            <forward name="4" path="/questionario/questionarioCmpAtendimento.jsp" />
        </action>
        <!-- /questionario fim -->
    </action-mappings>
    <controller nocache="true" processorClass="com.indracompany.struts.annotations.RequestProcessor" />
    <!-- <controller processorClass="org.apache.struts.tiles.TilesRequestProcessor"/> -->
    <message-resources parameter="com/indracompany/actions/ApplicationResource" />
</struts-config>