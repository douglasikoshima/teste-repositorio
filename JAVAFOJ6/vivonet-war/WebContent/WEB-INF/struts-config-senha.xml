<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
              "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
              "struts-config_1_2.dtd">
<struts-config>
    <form-beans>
        <form-bean name="gestaoSenhaForm" type="senha.gestaoSenha.GestaoSenhaController$GestaoSenhaForm" />
        <form-bean name="historicoConsultarForm" type="senha.historicoSenha.HistoricoSenhaController$HistoricoConsultarForm" />
        <form-bean name="mostrarNavegacaoURAForm" type="senha.mostrarNavegacaoURA.MostrarNavegacaoURAController$MostrarNavegacaoURAForm" />
        <form-bean name="validarClienteForm" type="senha.validarCliente.ValidarClienteController$ValidarClienteForm" />
    </form-beans>
    <action-mappings>
        <!-- /senha/gestaoSenha -->
        <action path="/senha/gestaoSenha/begin" type="senha.gestaoSenha.GestaoSenhaController" parameter="begin"
            scope="request">
            <forward name="success" path="/senha/gestaoSenha/gestaoSenha.jsp" />
        </action>
        <action path="/senha/gestaoSenha/gerarSenhaAction" type="senha.gestaoSenha.GestaoSenhaController"
            parameter="gerarSenhaAction" scope="request" name="gestaoSenhaForm">
            <forward name="success" path="/senha/gestaoSenha/currentPage.do" />
        </action>
        <action path="/senha/gestaoSenha/alterarSenhaAction" type="senha.gestaoSenha.GestaoSenhaController"
            parameter="alterarSenhaAction" scope="request" name="gestaoSenhaForm">
            <forward name="success" path="/senha/gestaoSenha/currentPage.do" />
        </action>
        <action path="/senha/gestaoSenha/validarSenhaAction" type="senha.gestaoSenha.GestaoSenhaController"
            parameter="validarSenhaAction" scope="request" name="gestaoSenhaForm">
            <forward name="success" path="/senha/gestaoSenha/currentPage.do" />
        </action>
        <action path="/senha/gestaoSenha/reinicializarSenhaAction" type="senha.gestaoSenha.GestaoSenhaController"
            parameter="reinicializarSenhaAction" scope="request" name="gestaoSenhaForm">
            <forward name="success" path="/senha/gestaoSenha/currentPage.do" />
        </action>
        <!-- /senha/gestaoSenha fim -->
        <!-- /senha/historicoSenha -->
        <action path="/senha/historicoSenha/begin" type="senha.historicoSenha.HistoricoSenhaController"
            parameter="begin" scope="request" name="historicoConsultarForm">
            <forward name="success" path="/senha/historicoSenha/historicoConsultarAction.do" />
        </action>
        <action path="/senha/historicoSenha/historicoConsultarAction" type="senha.historicoSenha.HistoricoSenhaController"
            parameter="historicoConsultarAction" scope="request" name="historicoConsultarForm">
            <forward name="success" path="/senha/historicoSenha/historicoSenha.jsp" />
        </action>
        <action path="/senha/historicoSenha/solicitarSenhaAction" type="senha.historicoSenha.HistoricoSenhaController"
            parameter="solicitarSenhaAction" scope="request" name="historicoConsultarForm">
            <forward name="success" path="/senha/historicoSenha/historicoSenha.jsp" />
        </action>
        <action path="/senha/historicoSenha/reinicializarFraseAction" type="senha.historicoSenha.HistoricoSenhaController"
            parameter="reinicializarFraseAction" scope="request" name="historicoConsultarForm">
            <forward name="success" path="/senha/historicoSenha/historicoSenha.jsp" />
        </action>
        <!-- /senha/historicoSenha fim -->
        <!-- /senha/mostrarNavegacaoURA -->
        <action path="/senha/mostrarNavegacaoURA/begin" type="senha.mostrarNavegacaoURA.MostrarNavegacaoURAController"
            parameter="begin" scope="request" name="mostrarNavegacaoURAForm">
            <forward name="success" path="/senha/mostrarNavegacaoURA/navegacaoURA.jsp" />
        </action>
        <!-- /senha/mostrarNavegacaoURA fim -->
        <!-- /senha/transferenciaChamada -->
        <action path="/senha/transferenciaChamada/begin" type="senha.transferenciaChamada.TransferenciaChamadaController"
            parameter="begin" scope="request">
            <forward name="success" path="/senha/transferenciaChamada/transferenciaChamada.jsp" />
        </action>
        <!-- /senha/transferenciaChamada fim -->
        <!-- /senha/validarCliente -->
        <action path="/senha/validarCliente/begin" type="senha.validarCliente.ValidarClienteController"
            parameter="begin" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/receberAction.do" />
        </action>
        <action path="/senha/validarCliente/carregaGrupo" type="senha.validarCliente.ValidarClienteController"
            parameter="carregaGrupo" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/iCampanha.jsp" />
        </action>
        <action path="/senha/validarCliente/receberAction" type="senha.validarCliente.ValidarClienteController"
            parameter="receberAction" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/validarCliente.jsp" />
        </action>
        <action path="/senha/validarCliente/carregarLigacao" type="senha.validarCliente.ValidarClienteController"
            parameter="carregarLigacao" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/receberAction.do" />
        </action>
        <action path="/senha/validarCliente/transferirCarregar" type="senha.validarCliente.ValidarClienteController"
            parameter="transferirCarregar" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/iTransferir.jsp" />
        </action>
        <action path="/senha/validarCliente/idPosAction" type="senha.validarCliente.ValidarClienteController"
            parameter="idPosAction" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/validarCliente.jsp" />
        </action>
        <action path="/senha/validarCliente/transferirRamalAction" type="senha.validarCliente.ValidarClienteController"
            parameter="transferirRamalAction" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/validarCliente.jsp" />
        </action>
        <action path="/senha/validarCliente/transferirUraAction" type="senha.validarCliente.ValidarClienteController"
            parameter="transferirUraAction" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/validarCliente.jsp" />
        </action>
        <action path="/senha/validarCliente/terminarAction" type="senha.validarCliente.ValidarClienteController"
            parameter="terminarAction" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/validarCliente.jsp" />
        </action>
        <action path="/senha/validarCliente/iniciarChamada" type="senha.validarCliente.ValidarClienteController"
            parameter="iniciarChamada" scope="request" name="validarClienteForm">
            <forward name="success" path="/senha/validarCliente/validarCliente.jsp" />
        </action>
        <!-- /senha/validarCliente fim -->
    </action-mappings>
    <controller nocache="true" processorClass="com.indracompany.struts.annotations.RequestProcessor" />
    <!-- <controller processorClass="org.apache.struts.tiles.TilesRequestProcessor"/> -->
    <message-resources parameter="com/indracompany/actions/ApplicationResource" />
</struts-config>