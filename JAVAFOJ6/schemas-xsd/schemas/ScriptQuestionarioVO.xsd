<xs:schema targetNamespace="questionario.fo.vivo.com.br/vo" xmlns:tns="questionario.fo.vivo.com.br/vo" xmlns:xxl="questionario.fo.vivo.com.br/vo" xmlns:xs="http://www.w3.org/2001/XMLSchema">
