			<xs:documentation>VO que contem os atributos das ofertas e ofertas de Execao</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="idCaracOfertaAceita" type="xs:string"/>
