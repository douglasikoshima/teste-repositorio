				<xs:element name="dataHora" type="xs:string"/>
                <xs:element name="linha" type="xs:string"/>				
				<xs:element name="intencao" type="xs:string"/>
				<xs:element name="destino" type="xs:string"/>
				<xs:element name="idOfertaAceita" type="xs:string"/>
				<xs:element name="nmOfertaAceita" type="xs:string"/>				
			</xs:sequence>
		</xs:complexType>
	</xs:element>
