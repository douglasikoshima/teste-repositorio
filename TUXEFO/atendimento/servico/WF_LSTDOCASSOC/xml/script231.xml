
#import WFDocumentoTecnico:WFSelDocumentos;



string txt;
string compara;
DOMNODE parametro;
XMLGen Saida;

      txt  = "<ListaDocumentosTecnicosVO>\n";
      
      compara = XML:idDocTecTipo;
      if ( compara != "" )
         txt += "<idDocumentoTecnicoTipo>" + XML:idDocTecTipo + "</idDocumentoTecnicoTipo>\n";
      fi;
      
      compara = XML:nrDocumentoTecnico;
      if ( compara != "" )
         txt += "<nrDocumento>" + XML:nrDocumentoTecnico + "</nrDocumento>\n";
      fi;
      
      compara = XML:idDocTecEstado;
      if ( compara != "" )
         txt += "<inEstadoTecnico>" + XML:idDocTecEstado + "</inEstadoTecnico>\n";
      fi;
      
      compara = XML:dtPeriodo1;
      if ( compara != "" )
         txt += "<dtPeriodo1>" + XML:dtPeriodo1 + "</dtPeriodo1>\n";
      fi;

      compara = XML:dtPeriodo2;
      if ( compara != "" )
         txt += "<dtPeriodo2>" + XML:dtPeriodo2 + "</dtPeriodo2>\n";
      fi;

      compara = XML:comentario;
      if ( compara != "" )
         txt += "<ComentarioAbertura>" + XML:comentario + "</ComentarioAbertura>\n";
      fi;
      
      compara = XML:associados;
      if ( compara != "" )
         txt += "<Associados>" + XML:associados + "</Associados>\n";
      fi;
      txt += "</ListaDocumentosTecnicosVO>\n";
      
      parametro = txt;
      Saida = WFDocumentoTecnico.WFSelDocumentos( parametro );

return Saida;



      

            