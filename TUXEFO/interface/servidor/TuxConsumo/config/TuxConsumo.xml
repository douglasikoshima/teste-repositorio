<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
	<servers>
		<server name="TuxConsumo">
			<name>TuxConsumo</name>
			<logLevel>0</logLevel>
			<logFilePath>../log/</logFilePath>
			<logPrefix>TuxConsumo</logPrefix>
			<services>
				<service>
						<name>ATLYSHTTP</name>
						<type>sync</type>
						<tuxService>ATLYSHTTP</tuxService>
						<timeout>0</timeout>
						<flags>
								<flag>TPNOTRAN</flag>
						</flags>
				</service>
				<service>
					<name>DREGCONTATOA</name>
					<type>sync</type>
					<tuxService>REGCONTATOA</tuxService>
					<timeout>0</timeout>
					<flags>
						<flag>TPNOTRAN</flag>
					</flags>
				</service>
			</services>
		</server>
	</servers>
</config>
