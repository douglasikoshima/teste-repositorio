<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
        <servers>
                <server name="srvprepago">
                        <name>srvprepago</name>
                        <logLevel>0</logLevel>
                        <logFilePath>../log/</logFilePath>
                        <logPrefix>srvprepago</logPrefix>
                        <services>
                                <service>
                                        <name>DTuxProxyBE</name>
                                        <type>sync</type>
                                        <tuxService>TuxProxyBE</tuxService>
                                        <timeout>0</timeout>
                                        <flags>
                                                <flag>TPNOTRAN</flag>
                                        </flags>
                                </service>
                                <service>
                                        <name>DTUXINTERBE</name>
                                        <type>sync</type>
                                        <tuxService>TUXINTERBE</tuxService>
                                        <timeout>0</timeout>
                                        <flags>
                                                <flag>TPNOTRAN</flag>
                                        </flags>
                                </service>
                                <service>
                                        <name>DTUXPREPAGOBE</name>
                                        <type>sync</type>
                                        <tuxService>TUXPREPAGOBE</tuxService>
                                        <timeout>0</timeout>
                                        <flags>
                                                <flag>TPNOTRAN</flag>
                                        </flags>
                                </service>
                        </services>
                </server>
        </servers>
</config>
